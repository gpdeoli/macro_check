{
  "version": 2,
  "buildCommand": "npm run prisma:generate && npm run build",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "dist",
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}

